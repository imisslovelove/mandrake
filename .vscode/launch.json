  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "Debug Web",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "7777"
      ]
    },
    {
      "name": "Debug Web Skia",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--release",
        "--dart-define=FLUTTER_WEB_USE_SKIA=true"
      ]
    },
    {
      "name": "Profile",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    }
  ]
}
